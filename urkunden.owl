@prefix : <http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dipl: <http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik> .

<http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#dipl:vidID
dipl:dipl:vidID rdf:type owl:AnnotationProperty ;
                rdfs:comment "Verweis auf den der Klasse entsprechenden Eintrag im Vocabulaire international de la diplomatique (https://www.cei.lmu.de/VID/)."@de .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#ausgefertigtIn
dipl:ausgefertigtIn rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain dipl:Konzept ;
                    rdfs:range dipl:Original ;
                    rdfs:comment "Verbindet ein Konzept mit dessen tatsächlicher Ausfertigung in der Form eines oder mehrerer Originale."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#befestigtMit
dipl:befestigtMit rdf:type owl:ObjectProperty ,
                           owl:AsymmetricProperty ;
                  rdfs:domain dipl:Siegel ;
                  rdfs:range dipl:Siegelbefestigung ;
                  rdfs:comment "Art und Weise der Siegelbefestigung, meist Schnur, Lederstreifen oder Durchdruck."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#beglaubigtDurch
dipl:beglaubigtDurch rdf:type owl:ObjectProperty ;
                     owl:inverseOf dipl:bezeugtEchtheitVon ;
                     rdfs:domain dipl:Dokument ;
                     rdfs:range dipl:Beglaubigungsmittel ;
                     rdfs:comment "Verbindung zum Beglaubigungsmittel, welches dem Dokument seinen Rechtsstatus verleiht."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#begründet
dipl:begründet rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf dipl:repräsentiert ;
               rdfs:range dipl:Rechtsinhalt ;
               rdfs:comment "Beschreibt Urkunden, die Rechtsinhalte begründen."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#bestehtAusMaterial
dipl:bestehtAusMaterial rdf:type owl:ObjectProperty ;
                        rdfs:range dipl:Material ;
                        rdfs:comment "Anwendbar sowohl auf den Beschreibstoff der Urkunde als auch das Sigel."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#beurkundet
dipl:beurkundet rdf:type owl:ObjectProperty ;
                rdfs:domain dipl:Aussteller ;
                rdfs:range dipl:Rechtsinhalt .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#bezeugt
dipl:bezeugt rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf dipl:repräsentiert ;
             rdfs:range dipl:Rechtsinhalt ;
             rdfs:comment "Beschreibt Urkunden, die Rechtsinhalte bezeugen, aber nicht selbst begründen."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#bezeugtEchtheitVon
dipl:bezeugtEchtheitVon rdf:type owl:ObjectProperty ;
                        rdfs:domain dipl:Beglaubigungsmittel ;
                        rdfs:range dipl:Dokument .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#enthaltenIn
dipl:enthaltenIn rdf:type owl:ObjectProperty ;
                 owl:inverseOf dipl:enthältMerkmal .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#enthältFormel
dipl:enthältFormel rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf dipl:enthältMerkmal ;
                   rdfs:range dipl:Formel .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#enthältGrafik
dipl:enthältGrafik rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf dipl:enthältMerkmal ;
                   rdfs:range dipl:GrafischesSymbol .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#enthältMerkmal
dipl:enthältMerkmal rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdf:type owl:TransitiveProperty ;
                    rdfs:range dipl:Merkmal ;
                    rdfs:comment "Dient zur Nennung innerer und äußerer Merkmale eines Dokuments, und transitiv Merkmalen innerhalb anderer Merkmale im Dokument (z.B. Dokument enthält Formular enthält Dispositio)."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#erwähntOrt
dipl:erwähntOrt rdf:type owl:ObjectProperty ;
                rdfs:domain dipl:Dokument ;
                rdfs:range dipl:Ort .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#erwähntPerson
dipl:erwähntPerson rdf:type owl:ObjectProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( dipl:Dokument
                                               dipl:Formel
                                             )
                               ] ;
                   rdfs:range dipl:Rolle ;
                   rdfs:comment "Nennungen von Personen im Urkundentext."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#hatAussteller
dipl:hatAussteller rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf dipl:erwähntPerson ;
                   rdf:type owl:InverseFunctionalProperty ;
                   rdfs:range dipl:Rolle .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#hatAusstellungsort
dipl:hatAusstellungsort rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf dipl:erwähntOrt ;
                        rdfs:domain dipl:Dokument ;
                        rdfs:range dipl:Ausstellungsort .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#hatEmpfänger
dipl:hatEmpfänger rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf dipl:erwähntPerson .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#hatVorurkunde
dipl:hatVorurkunde rdf:type owl:ObjectProperty ;
                   rdfs:domain dipl:Dokument ;
                   rdfs:range dipl:Dokument ;
                   rdfs:comment "Verbindet ein Dokument mit einem früheren Dokument, welches als Vorlage diente."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#inseriert
dipl:inseriert rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf dipl:istAbschriftVonOriginal ;
               rdfs:domain dipl:Transsumpt ;
               rdfs:range dipl:Dokument ;
               rdfs:comment "Verbindung von Transsumpt/Insert zum inserierten Original."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#istAbschriftVonOriginal
dipl:istAbschriftVonOriginal rdf:type owl:ObjectProperty ;
                             owl:inverseOf dipl:istOriginalZuAbschrift ;
                             rdf:type owl:FunctionalProperty ;
                             rdfs:domain dipl:Abschrift ;
                             rdfs:range dipl:Original ;
                             rdfs:comment "Verbindung von einer Kopie zu ihrem Original."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#istOriginalZuAbschrift
dipl:istOriginalZuAbschrift rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf owl:topObjectProperty ;
                            rdf:type owl:InverseFunctionalProperty ;
                            rdfs:domain dipl:Original ;
                            rdfs:range dipl:Abschrift .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#istRepräsentiertDurch
dipl:istRepräsentiertDurch rdf:type owl:ObjectProperty ;
                           owl:inverseOf dipl:repräsentiert ;
                           rdfs:domain dipl:Rechtsinhalt ;
                           rdfs:range dipl:Urkunde .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#manifestiertAls
dipl:manifestiertAls rdf:type owl:ObjectProperty ;
                     rdfs:domain dipl:Urkunde ;
                     rdfs:range dipl:Manifestation ;
                     rdfs:comment "Die Art und Weise, in der ein Dokument erhalten ist."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#nenntZeugen
dipl:nenntZeugen rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf dipl:erwähntPerson ;
                 rdfs:range dipl:Zeuge .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#regestiert
dipl:regestiert rdf:type owl:ObjectProperty ;
                rdfs:domain dipl:Regest ;
                rdfs:range dipl:Rechtsinhalt .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#repräsentiert
dipl:repräsentiert rdf:type owl:ObjectProperty ;
                   rdfs:domain dipl:Urkunde ;
                   rdfs:range dipl:Rechtsinhalt ;
                   rdfs:comment "Verbindet abstrakten Rechtsinhalt mit einer konkreten Handlung oder einem konkreten Dokument, welches diesem Form gibt."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#sammelt
dipl:sammelt rdf:type owl:ObjectProperty ;
             rdfs:domain dipl:Urkundensammlung ;
             rdfs:range dipl:Urkunde ;
             rdfs:comment "Verwendet für die verschiedenen Arten von Urkundensammlungen, welche Urkunden gemäß unterschiedlicher Kriterien zusammenbringen."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#vidimiert
dipl:vidimiert rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf dipl:istAbschriftVonOriginal ;
               rdfs:domain dipl:Vidimus ;
               rdfs:range dipl:Dokument ;
               rdfs:comment "Verbindung vom Vidimus zum vidimierten Original."@de .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdf:type owl:ObjectProperty ,
                               owl:TransitiveProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#datiert
dipl:datiert rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:dateTime .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#datiertActum
dipl:datiertActum rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf dipl:datiert .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#datiertDatum
dipl:datiertDatum rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf dipl:datiert .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#enthältText
dipl:enthältText rdf:type owl:DatatypeProperty ;
                 rdfs:domain dipl:Formel ;
                 rdfs:range xsd:string .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#gültigBis
dipl:gültigBis rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain dipl:Urkunde ;
               rdfs:range [ rdf:type rdfs:Datatype ;
                            owl:oneOf [ rdf:type rdf:List ;
                                        rdf:first "beschränkt" ;
                                        rdf:rest [ rdf:type rdf:List ;
                                                   rdf:first "dauerhaft" ;
                                                   rdf:rest rdf:nil
                                                 ]
                                      ]
                          ] ;
               rdfs:comment "Dient zur Unterscheidung zwischen Urkunden, welche eine zeitlich eingeschränkte Funktion haben (z.B. Mandate), und Urkunden, welche dauerhafte neue Rechtszustände begründen (z.B. Diplome)."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#verfasstInForm
dipl:verfasstInForm rdf:type owl:DatatypeProperty ;
                    rdfs:range [ rdf:type rdfs:Datatype ;
                                 owl:oneOf [ rdf:type rdf:List ;
                                             rdf:first "objektiv" ;
                                             rdf:rest [ rdf:type rdf:List ;
                                                        rdf:first "subjektiv" ;
                                                        rdf:rest rdf:nil
                                                      ]
                                           ]
                               ] ;
                    rdfs:comment "Unterscheidet zwischen objektiver (dritte Person) und subjektiver (erste Person) Fassung."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#verfasstInSprache
dipl:verfasstInSprache rdf:type owl:DatatypeProperty ;
                       rdfs:domain dipl:Dokument ;
                       rdfs:range xsd:language .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Abschrift
dipl:Abschrift rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty dipl:istAbschriftVonOriginal ;
                                     owl:someValuesFrom dipl:Original
                                   ] ;
               rdfs:subClassOf dipl:Manifestation ;
               dipl:dipl:vidID "vid:VID_53" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Adressat
dipl:Adressat rdf:type owl:Class ;
              rdfs:subClassOf dipl:Rolle ;
              dipl:dipl:vidID "vid:VID_18" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Apprecatio
dipl:Apprecatio rdf:type owl:Class ;
                rdfs:subClassOf dipl:Formel ;
                dipl:dipl:vidID "vid:VID_251" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Arenga
dipl:Arenga rdf:type owl:Class ;
            rdfs:subClassOf dipl:Formel ;
            dipl:dipl:vidID "vid:VID_196bis" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Ausfertigungsform
dipl:Ausfertigungsform rdf:type owl:Class ;
                       rdfs:subClassOf dipl:Manifestation .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Aussteller
dipl:Aussteller rdf:type owl:Class ;
                rdfs:subClassOf dipl:Rolle ;
                dipl:dipl:vidID "vid:VID_16" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Ausstellungsort
dipl:Ausstellungsort rdf:type owl:Class ;
                     rdfs:subClassOf dipl:Ort .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#BeglaubigteAbschrift
dipl:BeglaubigteAbschrift rdf:type owl:Class ;
                          rdfs:subClassOf dipl:Abschrift ;
                          owl:disjointWith dipl:EinfacheAbschrift ;
                          dipl:dipl:vidID "vid:VID_54" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Beglaubigungsmittel
dipl:Beglaubigungsmittel rdf:type owl:Class ;
                         rdfs:subClassOf dipl:ÄußeresMerkmal ;
                         dipl:dipl:vidID "vid:VID_9a" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Beschreibstoff
dipl:Beschreibstoff rdf:type owl:Class ;
                    rdfs:subClassOf dipl:Material ;
                    dipl:dipl:vidID "vid:VID_133" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Blei
dipl:Blei rdf:type owl:Class ;
          rdfs:subClassOf dipl:Metall .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Breve
dipl:Breve rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( dipl:Dokument
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty dipl:bezeugt ;
                                                        owl:someValuesFrom dipl:Rechtsinhalt
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf dipl:Papsturkunde ;
           dipl:dipl:vidID "vid:VID_406" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Bulle
dipl:Bulle rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( dipl:Dokument
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty dipl:beglaubigtDurch ;
                                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass dipl:Siegelbulle
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty dipl:hatAussteller ;
                                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass dipl:Papst
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf dipl:Dokument ;
           dipl:dipl:vidID "vid:VID_398" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Carta
dipl:Carta rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( dipl:Dokument
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty dipl:begründet ;
                                                        owl:someValuesFrom dipl:Rechtsinhalt
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty dipl:verfasstInForm ;
                                                        owl:hasValue "subjektiv"
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf dipl:Form ;
           rdfs:comment "Urkunde die Präsens und der ersten Person verfasst ist, und in der der Aussteller ausdrücklich eine Verpflichtung übernimmt (Härtel 2011, S. 25-26)."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Chirograph
dipl:Chirograph rdf:type owl:Class ;
                rdfs:subClassOf dipl:Beglaubigungsmittel ;
                dipl:dipl:vidID "vid:VID_44" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Corroboratio
dipl:Corroboratio rdf:type owl:Class ;
                  rdfs:subClassOf dipl:Formel ;
                  dipl:dipl:vidID "vid:VID_246" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Deperditum
dipl:Deperditum rdf:type owl:Class ;
                rdfs:subClassOf dipl:Urkunde ;
                rdfs:comment "Urkunde, welche nicht selbst erhalten ist, sondern aus Referenzen anderer Quellen bekannt ist."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Diktator
dipl:Diktator rdf:type owl:Class ;
              rdfs:subClassOf dipl:Kanzleipersonal ;
              dipl:dipl:vidID "vid:VID_272" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Diplom
dipl:Diplom rdf:type owl:Class ;
            rdfs:subClassOf dipl:Dokument ,
                            [ owl:intersectionOf ( dipl:Dokument
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty dipl:gültigBis ;
                                                     owl:hasValue "beschränkt"
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
            dipl:dipl:vidID "vid:VID_386" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Dispositio
dipl:Dispositio rdf:type owl:Class ;
                rdfs:subClassOf dipl:Formel ;
                dipl:dipl:vidID "vid:VID_198" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Dokument
dipl:Dokument rdf:type owl:Class ;
              rdfs:subClassOf dipl:Urkunde ;
              rdfs:comment "Ein materielles Schriftstück."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Duplikat
dipl:Duplikat rdf:type owl:Class ;
              rdfs:subClassOf dipl:Mehrfachausfertigung ;
              dipl:dipl:vidID "vid:VID_45" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#EinfacheAbschrift
dipl:EinfacheAbschrift rdf:type owl:Class ;
                       rdfs:subClassOf dipl:Abschrift ;
                       dipl:dipl:vidID "vid:VID_55" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Eingangsprotokoll
dipl:Eingangsprotokoll rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( dipl:InneresMerkmal
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty dipl:enthältFormel ;
                                                                    owl:allValuesFrom [ rdf:type owl:Class ;
                                                                                        owl:unionOf ( dipl:Inscriptio
                                                                                                      dipl:Intitulatio
                                                                                                      dipl:Invocatio
                                                                                                    )
                                                                                      ]
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       dipl:dipl:vidID "vid:VID_182" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Elongata
dipl:Elongata rdf:type owl:Class ;
              rdfs:subClassOf dipl:Schrift ;
              dipl:dipl:vidID "vid:VID_160" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Empfänger
dipl:Empfänger rdf:type owl:Class ;
               rdfs:subClassOf dipl:Rolle ;
               owl:disjointWith dipl:Zeuge ;
               dipl:dipl:vidID "vid:VID_17" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Empfängerausfertigung
dipl:Empfängerausfertigung rdf:type owl:Class ;
                           rdfs:subClassOf dipl:Ausfertigungsform ;
                           owl:disjointWith dipl:Kanzleiausfertigung ;
                           dipl:dipl:vidID "vid:VID_267" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Entwurf
dipl:Entwurf rdf:type owl:Class ;
             rdfs:subClassOf dipl:Manifestation ;
             dipl:dipl:vidID "vid:VID_349" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Eschatokoll
dipl:Eschatokoll rdf:type owl:Class ;
                 rdfs:subClassOf dipl:InneresMerkmal ;
                 dipl:dipl:vidID "vid:VID_182a" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Form
dipl:Form rdf:type owl:Class ;
          rdfs:comment "Bezieht sich auf die formalen Aspekte, in der die Urkunden verfasst ist, nicht auf eine rechtliche Funktion."@de .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Formel
dipl:Formel rdf:type owl:Class ;
            rdfs:subClassOf dipl:InneresMerkmal ;
            dipl:dipl:vidID "vid:VID_183" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Formelsammlung
dipl:Formelsammlung rdf:type owl:Class ;
                    dipl:dipl:vidID "vid:VID_313" .


###  http://www.semanticweb.org/niklas/ontologies/2020/4/diplomatik#Formular
dipl:Formular rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( dipl:InneresMerkmal
                                                         [ owl:intersectionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                                         owl:onProperty dipl:enthältFormel ;
                                                                                                         owl:someValuesFrom dipl:Eschatokoll
                                                                                                       ]
                                                                                                       [ rdf:type owl:Restriction ;
                                                                                                         owl:onProperty dipl:enthältFormel ;
